package at.gv.egiz.android.application;

import android.app.Application;
import at.gv.egiz.android.communication.P2PCommunicationDevice;

public class LocationProverApplication extends Application {

	private P2PCommunicationDevice communicationDevice = null;
	private String remoteMacAddress = null;

	public P2PCommunicationDevice getCommunicationDevice() {
		return communicationDevice;
	}

	public void setCommunicationDevice(
			P2PCommunicationDevice communicationDevice) {
		this.communicationDevice = communicationDevice;
	}

	public String getRemoteMacAddress() {
		return remoteMacAddress;
	}

	public void setRemoteMacAddress(String remoteMacAddress) {
		this.remoteMacAddress = remoteMacAddress;
	}

}
